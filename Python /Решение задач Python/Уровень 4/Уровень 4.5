1. # Выведите в консоль все числа в промежутке от 10 до 1000, у которых последняя цифра четная.

n = 10

result = []

while n <=1000:
    string = str(n)
    if int(string[-1])%2 ==0:
        result.append(n)
    n += 1
        
print(result)

# 2. Дан словарь 
# dict = {1:{1:{1:111,2:112,3:113}, 2:{1:121,2:122,3:123}}, 2:{1:{1:211,2:212,3:213}, 2:{1:221,2:222,3:223}}, 3:{1:311,2:312,3:313}, 
# 2:{1:321,2:322,3:323}}. Найдите сумму элементов этого словаря.

dict = {1:{
           1:{
               1:111,2:112,3:113
               }, 
           2:{
               1:121,2:122,3:123
               }
           }, 
        2:{
            1:{
                1:211,2:212,3:213
                }, 
            2:{
                1:221,2:222,3:223
                }
            },
        3:{
            1:{
                1:311,2:312,3:313
            }, 
            2:{
                1:321,2:322,3:323
                }
            }
        }

dict_iter1 = {}
dict_iter2 = {}

sum = 0

for i in dict.values():
    dict_iter1 = i
    for i in dict_iter1.values():
        dict_iter2 = i
        for i in dict_iter2.values():
            sum += i
            
