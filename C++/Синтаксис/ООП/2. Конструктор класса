#include<iostream>
#include<string>

using namespace std;

class Point
{
private:

	int x;
	int y;

public:

	Point(int value_x, int value_y)
	{
		x = value_x;
		y = value_y;
	}

	int Print()
	{
		cout << "x = " << x << "\ty= "<< y << endl;
		return 0;
	}
};

int main()
{
	setlocale(LC_ALL, "ru");

	Point a(5,44);

	a.Print();

	return 0;
}
